<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.examenparcial2_dsm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExamenParcial2_DSM">
        <activity android:name=".CarritoActivity"></activity>
        <activity android:name=".ProductoActivity" />
        <activity android:name=".ListadoProductos" />
        <activity android:name=".PrincipalActivity" />
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".InicioSesion">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InicioSesion" />
        </activity>
        <activity android:name=".InicioSesion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>